{
  "name": "content-marketing-toolkit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:stats": "vite build --mode production && echo 'Build complete! Check bundle sizes below:' && ls -lah dist/assets/js/ 2>/dev/null || ls -lah dist/assets/ 2>/dev/null || echo 'No JS assets found'",
    "analyze": "npx vite-bundle-analyzer dist",
    "size-check": "ls -lah dist/assets/js/ 2>/dev/null | grep -E '\\.(js)$' | sort -k5 -hr || echo 'No JS files found. Run npm run build first.'",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "14": "^1.0.1",
    "@supabase/supabase-js": "^2.49.7",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.9.0",
    "lucide-react": "^0.511.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^4.0.11",
    "react-router-dom": "^7.6.0",
    "reconnecting-websocket": "^4.4.0",
    "tiktoken": "^1.0.21",
    "use-debounce": "^9.0.4",
    "uuid": "^11.1.0",
    "ws": "^8.18.2",
    "youtube-transcript": "^1.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.10.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.39.2",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^0.21.0"
  }
}
